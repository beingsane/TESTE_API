html				{width:100%;height:100%;}body				{width:100%;height:100%;font-family:'Open Sans',sans-serif;background:#fff;color:#555;overflow: hidden;}.left				{float:left;}.right				{float:right;}.clear				{clear:both;}.pointer			{cursor: pointer;}.redIcon:before		{color: #d9534f}#site				{height: 100%;width: 100%;margin: 0 auto;min-width: 400px}#logo				{color: #dedede;font-size: 20pt;text-transform: uppercase;font-weight: 700;text-decoration: none;height: 42px;width: 90px;padding-left: 17px;display: table-cell;vertical-align: middle;}#logo span			{display: block;font-size: 6pt;}#logo_site			{text-align: center;}#logo_site img		{max-width: 225px}.top				{background-color: #272729;box-shadow: 0 3px 8px rgba(0,0,0,0.1);margin: 0;padding: 0;min-height: 42px;}#menuLeft			{position:absolute; width: 225px;border-right: 1px solid #d0d0d0;box-shadow: -2px 0 5px rgba(0,0,0,0.1) inset;height: 100%;overflow: hidden;}#menuLat			{overflow-y: auto;overflow-x: hidden;background: #F8F8F8}#contentRight		{margin-left: 226px;display: block;}#actionMenu			{padding-left: 15px;cursor: pointer;}#nameUser			{padding-top: 16px;padding-right: 6px;color: #fff;font-size: 12px;text-align: right;}#actionUser			{cursor: pointer;}#content			{overflow: auto;padding: 6px;}.filter 			{border: 1.5px solid #ccc;margin-bottom: 10px;padding: 5px;}.heightIcon			{height: 30px}.notClassDir td		{background: #d9534f!important;color:#fff }/* Tollbar */#tollbar			{height: auto;display: block;text-align: right;padding: 3px 5px 6px 0;border-bottom: solid 1px #ccc;}#mensTollbar		{padding:10px;}#mensTollbar span	{padding-top: 6px}.iconTollbar		{padding:0 5px 0 5px;display:inline-block;cursor:pointer;}.btnTollbar			{background: #37a6cd;border: 1px solid #2a87a7;color: #fff;text-shadow: 0 1px 0 rgba(0,0,0,0.5);border-color: #357ebd;margin-bottom: 0;font-size: 14px;text-align: center;white-space: nowrap;cursor: pointer;border-radius: 4px;height: 26px;margin-top: 3px;padding: 10px 10px 0px 50px;margin-right: 6px;}.btnTollbar:before	{color: #ccc;padding-top: 6px;padding-left: 8px;border-right: solid 1px #000;height: 30px;padding-right: 8px;}.btnTollbar:hover	{text-decoration: underline;}/* Menu Lateral */.aba_sys			{display: block;width: auto;padding: 0px 0px 0px 40px;margin: 0;color: #222;height: 38px;line-height: 38px;text-decoration: none;font-size: 16px;border-right: 4px solid #EFEFEF;background: #EFEFEF}.aba_sys:before		{margin-left: 12px;margin-top: 10px;font: 17px/1em 'Glyphicons Regular';}.li_menu_sys ul		{background: #fff;}.aba_active			{background: #ececec;border-right: 4px solid #37a6cd;}.activeUl			{border-right: 4px solid #37a6cd;}.menu_sys			{display: block;width: auto;padding: 0px 0px 0px 20px;margin: 0;color: #222;height: 24px;line-height: 24px;font-size: 14px;}.menu_sys:last-child{border-bottom: solid 1px #ccc}.menu_sys a			{text-decoration: none;color: #000;display: block;}.menu_sys a:hover	{text-decoration: underline;}.active_menu		{font-weight: bold;}/* Formulário */input[type=text], input[type=password], select, textarea {border-color: #d8d9da;color: #000;-webkit-border-radius: 0;-moz-border-radius: 0;border-radius: 0;}select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {font-size: 13px;}input[readonly],input[disabled],select[readonly],select[disabled]		{background: #eee;color: #999;}.form-control 		{height: 18px;padding: 6px 12px;font-size: 14px;line-height: 1.428571429;color: #555;vertical-align: middle;background-color: #fff;border: 1px solid #ccc;-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);}.labelForm			{display: block;font-size: 18px;padding: 15px 0px 4px 2px;}.select				{height: 34px;}/* Table */.tableListDefault				{width:100%;border-collapse:separate;border:solid 1px #CCC;background:#E7E7E7;border-spacing:1px;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;}.tableListDefault th			{border-color: #37a6cd;background-color: #37a6cd;color: #fff;text-transform: uppercase;text-shadow: 0 1px 0 rgba(0,0,0,0.5);box-shadow: inset 1px 1px 1px rgba(255,255,255,0.2),inset -1px -1px 1px rgba(0,0,0,0.2);-moz-box-shadow: inset 1px 1px 1px rgba(255,255,255,0.2),inset -1px -1px 1px rgba(0,0,0,0.2);-webkit-box-shadow: inset 1px 1px 1px rgba(255,255,255,0.2),inset -1px -1px 1px rgba(0,0,0,0.2);padding: 4px}.tableListDefault td			{background:#fff;padding:3px;vertical-align: middle;}.tableListDefault tr:hover td	{background-color:#FFFFCC;}.colorTdBlue 	td				{background:#DBF0F7 }.row_gren 		td				{background: #EFE}/* Mensagens de erro */#maskError						{z-index: 1001;border-color: #e5e5e5;-webkit-border-radius: 0;-moz-border-radius: 0;border-radius: 0;background: #fff;box-shadow: none;-moz-box-shadow: none;-webkit-box-shadow: none;position: fixed;top: 0;right: 0;bottom: 0;left: 0;display: none;overflow: auto;overflow-y: scroll;opacity: 0.7;}#errorContent					{z-index: 1002;position: absolute;background-color: #fff;border: 1px solid #999;border-radius: 6px;-webkit-box-shadow: 0 3px 9px rgba(0,0,0,0.5);box-shadow: 0 3px 9px rgba(0,0,0,0.5);top: 100px;min-width: 300px;min-height: 100px;display: none;max-height: 80%;overflow: auto;}#topErrorContent				{color: #575655;min-height: 25px;border-bottom: solid 1px #000;}#topErrorContent h4				{font-size: 18px;padding-left: 4px;padding-top: 4px;}#closeError						{position: absolute;right: 10px;padding: 0;margin: 0;opacity: 1;cursor: pointer;background: transparent;border: 0;-webkit-appearance: none;font-size: 22px;}#centerErrorContent				{min-height: 30px;padding: 10px}#centerErrorContent div			{display: block;height: 32px;padding-top: 5px;}#bottomErrorContent				{background: #e5e5e5;border-top: solid 1px #000;min-height: 55px}#btnCloseError					{display: inline-block;padding: 6px 12px;margin-bottom: 0;font-size: 14px;font-weight: normal;line-height: 1.428571429;text-align: center;white-space: nowrap;vertical-align: middle;cursor: pointer;border-radius: 4px;border: 1px solid #ccc;margin: 10px;}#btnCloseError:hover			{background: #f4f4f4;}.list_icon						{width: 160px;display: inline-block;font-size: 12px;line-height: 48px;overflow: hidden;padding-left: 50px;text-align: left;height: 50px;border: 1px solid #efefef;margin: 0 5px 10px 0;padding-right: 10px;}.list_icon:before				{padding-left: 10px;padding-top: 12px;}.list_icon	strong				{padding-right: 5px}.list_icon	span				{display: none;}