div#Pagination_externaldiv{font-size: 14px;text-align: center;padding-top: 0px;}
div#Pagination_externaldiv table,  div#Pagination_externaldiv table tr, div#Pagination_externaldiv table tr td{background-color: inherit;	border: 0px;text-align:center;}
div#Pagination_externaldiv table tr td{background-color: inherit;}
div#Pagination_Pagination{display:inline-block;margin:0px 3px 0px 3px;}
div#Pagination_Pagination img{border:0px;margin:0px 3px 0px 3px;}
div#Pagination_DetailsOuter{text-align: center;margin-top: 8px;}
div#Pagination_Details{margin:3px auto;visibility:inherit;background-color: #D1D3D9;width: 450px;border-style: solid;border-color: #999999;border-width:1px;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;}
div#Pagination_DetailsInner select{margin-top:3px;font-size:10px;}
div#Pagination_Pagination a.img_link, div#Pagination_Pagination a.img_link:visited, div#Pagination_Pagination a.img_link:active {text-decoration:none;}
div#Pagination_Pagination a.n_link:link, div#Pagination_Pagination a.n_link:visited {text-decoration:none;color:#000;border:1.5px solid #DDD; -moz-border-radius: 4px;-webkit-border-radius: 4px;border-radius: 4px;background-color: #E7E7E7}
div#Pagination_Pagination a.n_link:hover, div#Pagination_Pagination a.n_link:active {background-color:#ccc;}
div#Pagination_DetailsInner{padding-bottom: 2px;}
.CurrentPage_pagination{background:#37a6cd!important; color: white!important;border-color: #37a6cd !important;}